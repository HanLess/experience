思路：
从0位起，将后面的数插入到前面排好序的数列中合适的位置

var my = function(arr){
    for(let i = 0;i < arr.length;i ++){
        for(let j = 0;j < i;j ++){   // 遍历 i 前面的一段数列，查到合适的位置
            if(arr[j] >= arr[i]){
                let t = arr[i]
                arr[i] = arr[j]
                arr[j] = t
            }
        }
    }

    return arr
}
